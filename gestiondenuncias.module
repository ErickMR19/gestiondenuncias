      $output .= '<p>' . t('Provee un tipo de contenido "Denuncia" y un tipo de comentario "Asignacion entidad"');
      $output .= t('Adem√°s provee una API rest para obtener/crear estos contenidos') . '</p>';

      return $output;

    default:
  }
}

